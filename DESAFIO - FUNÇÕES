// FUNÇÃO QUE SOMA TODOS OS NÚMEROS INTEIROS ANTES DO NÚMERO DECLARADO

let param1 = 0
let resultado = 0;

function soma_numeros(param1){
  var soma = 0;
  for(let i = 1; i <= param1; i++){
  soma += i;
  }
  return soma;
 }
param1 = parseInt(prompt("Digite um número inteiro: "));
soma_numeros(param1);
