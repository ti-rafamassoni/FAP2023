var palpite = 0
var premiado = 0
var cont = 0

premiado = Math.floor((Math.random() * 101)) + 1;


for (let i = 1; i <=5; i++){
  palpite = parseInt(prompt("Digite um número entre 1 e 100: "));
    if (palpite > premiado){
      console.log("Palpite muito alto. Tente novamente");
      } else if (palpite < premiado){
        console.log("Palpite muito baixo. Tente novamente");
      } else {
        console.log("ACERTOU!!! Você chegou ao número correto em " + i + " tentativas");
        break;
      }
    if (i == 5){
      console.log("Você chegou ao número total de tentativas. O número premiado era " + premiado);
    }
}
