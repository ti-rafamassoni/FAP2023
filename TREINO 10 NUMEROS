// Faça um programa em JS que leia um 10 números e verifique se existem valores já digitados e os escreva na tela. 
Dica: procurar pela função que pesquisa um valor no vetor e retorna a posição.
  
var lista = [];
var i = 0;
while(i < 10){
var num = parseInt(prompt("Digite um número inteiro, entre 1 e 100: "));
  if(num < 0 || num > 100){
  alert("Entrada Inválida");
  }else{
  var posicao = lista.indexOf(num);
    if(posicao != -1){
    alert("O número " + num + " já existe na posição " + posicao + " da lista.");
    }else{
    lista.push(num);
    i++;
    }
  }
}
console.log(lista);


