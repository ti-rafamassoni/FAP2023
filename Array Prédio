let predio = [
[2,4,5,3], // Moradores 1o Andar: Apartamentos 1, 2, 3 e 4;
[4,4,6,3], // Moradores 2o Andar: Apartamentos 1, 2, 3 e 4;
[8,1,3,3]  // Moradores 3o Andar: Apartamentos 1, 2, 3 e 4.
]
  for(var andar = 0; andar < predio.length; andar++){
    for(var apartamento = 0; apartamento < predio[andar].length; apartamento++){
    console.log(predio[andar][apartamento]);
    }
  }
